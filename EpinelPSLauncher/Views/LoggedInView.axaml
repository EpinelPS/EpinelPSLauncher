<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EpinelPSLauncher.Views.LoggedInView"
			 Loaded="UserControl_Loaded">
	<StackPanel>
		<TextBlock x:Name="Username"></TextBlock>
		<Button x:Name="PlayButton" Click="Play_Click" Padding="100,20,100,20">Play</Button>
		<TextBlock>Current version: 123.132.123</TextBlock>
		<Button x:Name="UpdateRepairButton" Click="UpdateOrRepair_Click">Update/repair</Button>
	</StackPanel>
</UserControl>
